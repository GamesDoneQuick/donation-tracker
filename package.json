{
  "name": "donation_tracker",
  "version": "0.1.0",
  "description": "GDQ Donation Tracker",
  "scripts": {
    "build": "git clean -fxd static && cross-env NODE_ENV=production webpack",
    "start": "cross-env NODE_ENV=${NODE_ENV:-development} SOURCE_MAPS=${SOURCE_MAPS:-1} webpack-dev-server",
    "test": "karma start --single-run",
    "test:watch": "karma start",
    "fix:prettier": "prettier --write \"bundles/**\"",
    "fix:lint": "yarn lint --fix",
    "fix:all": "yarn fix:prettier && yarn fix:lint",
    "lint": "eslint bundles spec --ext js --ext jsx --ext ts --ext tsx --quiet",
    "yarn:install": "yarn install",
    "tsc": "tsc --version && tsc",
    "tsc:watch": "tsc --version && tsc --watch"
  },
